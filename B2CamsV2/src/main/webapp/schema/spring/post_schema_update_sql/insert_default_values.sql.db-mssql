-- Populates the prefs table with default values if it has not been previously populated.
--
IF (SELECT COUNT(*) FROM bbdn_props) = 0
BEGIN
    INSERT INTO bb_cams_props (
		cams_enabled_ind,  
        cams_frequency,
        cams_user_id,
        cams_username,
        cams_password,
        cams_web_address
        )  
    Values (  
        'N', 
        '0',
        '1',
        'Default username',
        'Default password',
        'Default web address'
     );
commit;
END;

